map gb :buffer<Space>
" Code blocks (see below for more info)
getIP() -> {{
httpRequest({url: 'http://api.ipify.org/?format=json', json: true},
            function(res) { Status.setMessage('IPAA: ' + res.ip); });
}}
" Displays your public IP address in the status bar
map ci :call getIP<CR>
unmap d
map d closeTab
unmap X
map u lastClosedTab
" close and step left
map D d
unmap J K
"map J 
"map K
unmap h l
map h previousTab
map l nextTab
unmap H L
map H scrollLeft
map L scrollRight
map <C-o> goBack
map <C-i> goForward
map <C-u> scrollFullPageUp
map <C-d> scrollFullPageDown


